// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model sensordata {
  id          Int      @id @default(autoincrement())
  node        Int
  temperature Float?
  pressure    Float?
  humidity    Float?
  gas         Float?
  soil        Float?
  createdat   DateTime @default(now())
}
